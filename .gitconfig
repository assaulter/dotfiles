[user]
	name = Kazuki Kubo
	email = kz@assaulter.org
[core]
	excludesfile = /Users/assaulter/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
	f = fetch --prune
	cob = checkout -b
	s = status
	b = branch
	co = checkout
	ps = push
	ci = commit
	d = diff
	pl = pull
